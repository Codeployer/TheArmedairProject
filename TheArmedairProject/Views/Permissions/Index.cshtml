@model IEnumerable<TheArmedairProject.Models.PermissionsModels>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.PermName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.PermName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PermID }) |
            @Html.ActionLink("Details", "Details", new { id=item.PermID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PermID })
        </td>
    </tr>
}

</table>
